"generated":{"line":72,"column":3}},{"source":"src/API/CustomMap.ts","name":null,"original":{"line":90,"column":0},"generated":{"line":74,"column":0}},{"source":"src/API/CustomMap.ts","name":null,"original":{"line":90,"column":0},"generated":{"line":74,"column":9}},{"source":"src/API/CustomMap.ts","name":null,"original":{"line":90,"column":0},"generated":{"line":74,"column":18}},{"source":"src/API/CustomMap.ts","name":null,"original":{"line":24,"column":0},"generated":{"line":75,"column":0}},{"source":"src/API/CustomMap.ts","name":null,"original":{"line":24,"column":0},"generated":{"line":75,"column":1}},{"source":"src/API/CustomMap.ts","name":null,"original":{"line":24,"column":0},"generated":{"line":75,"column":3}},{"source":"src/API/CustomMap.ts","name":null,"original":{"line":24,"column":13},"generated":{"line":77,"column":0}},{"source":"src/API/CustomMap.ts","name":null,"original":{"line":24,"column":13},"generated":{"line":77,"column":7}},{"source":"src/API/CustomMap.ts","name":null,"original":{"line":24,"column":13},"generated":{"line":77,"column":8}},{"source":"src/API/CustomMap.ts","name":null,"original":{"line":24,"column":13},"generated":{"line":77,"column":17}},{"source":"src/API/CustomMap.ts","name":null,"original":{"line":24,"column":13},"generated":{"line":77,"column":20}},{"source":"src/API/CustomMap.ts","name":null,"original":{"line":24,"column":13},"generated":{"line":77,"column":29}},{"source":"src/API/CustomMap.ts","name":null,"original":{"line":91,"column":0},"generated":{"line":78,"column":0}}],"sources":{"src/API/CustomMap.ts":"// section typealias\n// #™━━━━━━━━━━━━━━━━━━━━━━━━\ntype GoogleMaps = google.maps.Map\nexport type GoogleMapMarker = google.maps.Marker\n// #™━━━━━━━━━━━━━━━━━━━━━━━━\n\n// section interface IMarkerType\n// #™━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━\n export interface IMarkerType {\n     //: - ©Global-PROPERTIES\n     // #™━━━━━━━━━━━━━━━━━━━━━━━━\n     location: {\n         lat: number,\n         lng: number\n     }\n     // #™━━━━━━━━━━━━━━━━━━━━━━━━\n     /// -™ Function to be implemented\n     markerContent(): string\n}\n// #™━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━\n\n\n// noinspection JSDeprecatedSymbols\nexport class CustomMap {\n    //: - ©Member-PROPERTIES\n    // #™━━━━━━━━━━━━━━━━━━━━━━━━\n    private readonly googleMap: GoogleMaps\n    // #™━━━━━━━━━━━━━━━━━━━━━━━━\n\n    /** ™- Constructor */\n    // ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━\n    constructor(divID: string) {\n        //..........\n        this.googleMap = new google.maps.Map(\n            document.getElementById(divID), {\n                //..........\n                zoom: 1,\n                center: {\n                    //..........\n                    lat: 0,\n                    lng: 0\n                }\n            })\n    }\n    /// - END OF: constructor\n    // ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━\n\n    // #━━━━━━━━━━━━━━━ methods ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━\n\n    addMarker(marker: IMarkerType): GoogleMapMarker {\n        /// -™ Destructuring\n        const { lat, lng } = marker.location\n        const { googleMap } = this\n\n        const markerEvent = new google.maps.Marker({\n            //..........\n            map: googleMap,\n            position: {\n                //..........\n                lat: lat,\n                lng: lng\n            }\n        })\n        /// - END OF: markerEvent\n\n        this.infoWindowContent(marker, markerEvent)\n        return markerEvent\n    }\n    /// - END OF METHOD: addMarker\n\n    /// -™ Helper function for addMarker\n    private infoWindowContent(marker: IMarkerType, markerEvent: GoogleMapMarker) {\n        /**━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━\n         * @EventListener:\n         * • Adding an event listener for when the\n         *   marker is clicked. Will display a popup\n         *   with the `content` added to the infoWindow\n         **━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━*/\n        markerEvent.addListener('click', () => {\n            //___________\n            const infoWindow = new google.maps.InfoWindow({\n                //..........\n                content: marker.markerContent()\n            })\n\n            infoWindow.open(this.googleMap, markerEvent)\n        })\n    }\n    /// - END OF: infoWindowContent\n}\n/** - END OF: @CustomMap */\n"},"lineCount":null}},"error":null,"hash":"4f96130f3866c72d14f6931a74a327b0","cacheData":{"env":{}}}